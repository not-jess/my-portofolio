{
    "routes": [
      {
        "src": "/css/(.*)", 
        "dest": "/css/$1"
      },
      {
        "src": "/js/(.*)", 
        "dest": "/js/$1"
      },
      {
        "src": "/images/(.*)", 
        "dest": "/images/$1"
      },
      {
        "src": "/(.*)", 
        "dest": "pages/index.html"
      }
    ]
  }
  